<h1>View Movies <%=  pluralize(@actors.count, "Actor") %></h1>
<table>
	<thead>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Actions</th>
	</thead>
	<tbody>
		<% @actors.each do |actor| %>
		<tr>
			<td><%= actor.first_name %></td>
			<td><%=actor.last_name %></td>
		</tr>
		<% end %>
	</tbody>
</table>
<a href="/movies">View Movie</a>